{
  "name": "k2gl/phpunit-fluent-assertions",
  "description": "Improves test code readability.",
  "homepage": "https://github.com/k2gl/phpunit-fluent-assertions",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "phpunit",
    "xunit",
    "testing"
  ],
  "authors": [
    {
      "name": "Nickolay Harin",
      "email": "l0gic@ya.ru"
    }
  ],
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "phpunit/php-code-coverage": "^9.2"
  },
  "autoload": {
    "psr-4": {
      "k2gl\\PHPUnitFluentAssertions\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  }
}
